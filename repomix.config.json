{
  "output": {
    "filePath": "repomix-output.xml",
    "style": "xml",
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 5,
    "showLineNumbers": false
  },
  "include": [],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "repomix.config.json",
      "repomix-output.*",
      "node_modules",
      "dist",
      ".env",
      "package-lock.json",
      "tsconfig.tsbuildinfo"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  }
}
